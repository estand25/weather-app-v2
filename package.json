{
  "name": "WeatherAppv2",
  "version": "1.0.0",
  "description": "Second version of Weather App",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --open",
    "coverage": "NODE_ENV=test jest --coverageReporters=lcov",
    "localcoverage": "jest --coverage  --coverageReporters=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estand25/quiz-builder-frontend.git"
  },
  "keywords": [],
  "author": "Standley Eugene",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/estand25/quiz-builder-frontend/issues"
  },
  "homepage": "https://github.com/estand25/quiz-builder-frontend#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "svg-url-loader": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@testing-library/react": "^10.2.1",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.0.1",
    "msw": "^0.19.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/path/to/dir/",
      "<rootDir>/index.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
