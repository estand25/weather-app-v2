{
  "name": "WeatherAppv2",
  "version": "1.0.0",
  "description": "Second version of Weather App",
  "main": "index.js",
  "scripts": {
    "create": "webpack",
    "dev": "webpack-dev-server --open",
    "start": "node server.js",
    "heroku-postbuild": "webpack -p",
    "format": "prettier --write \"src/**/*.js\"",
    "eslint-fix": "eslint --fix \"src/**/*.js\"",
    "build": "webpack --mode production",
    "coverage": "NODE_ENV=test jest --coverageReporters=lcov",
    "localcoverage": "jest --coverage  --coverageReporters=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estand25/weather-app-v2.git"
  },
  "keywords": [],
  "author": "Standley Eugene",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/estand25/weather-app-v2/issues"
  },
  "homepage": "https://github.com/estand25/weather-app-v2#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.17.1",
    "heroku": "^7.42.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@testing-library/react": "^10.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.2.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-svg-transformer": "^1.0.0",
    "less": "^3.11.3",
    "less-loader": "^6.1.1",
    "msw": "^0.19.3",
    "prettier": "2.0.5",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^6.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/path/to/dir/",
      "<rootDir>/index.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.svg": "<rootDir>/__mocks__/fileMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
